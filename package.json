{
	"name": "softwrap",
	"displayName": "softwrap",
	"description": "Toggles softwrap of lines. ",
	"version": "1.0.1",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/jsturtevant/vscode-softwrap/issues"
    },
    "homepage": "http://www.jamessturtevant.com/posts/Softwrap-a-Visual-Studio-Code-Extension/",
     "repository": {
        "type": "git",
        "url": "https://github.com/jsturtevant/vscode-softwrap"
    },
	"publisher": "jsturtevant",
	"engines": {
		"vscode": "^0.10.1"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.softwrap"
	],
	"main": "./out/src/extension",
	"contributes": {
		"commands": [{
			"command": "extension.softwrap",
			"title": "Softwrap"
		}]
	},
	"scripts": {
		"vscode:prepublish": "node ./node_modules/vscode/bin/compile",
		"compile": "node ./node_modules/vscode/bin/compile -watch -p ./"
	},
	"devDependencies": {
		"typescript": "^1.6.2",
		"vscode": "0.10.x"
	}
}